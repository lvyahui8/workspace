<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yahui.studyandroid"
    android:versionCode="1"
    android:versionName="1.0" xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" tools:ignore="OldTargetApi"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yahui.studyandroid.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.yahui.studyandroid.ImageViewActivity"
            android:label="@string/imageviewapp"
            android:launchMode="standard"
            ></activity>
        <activity 
            android:name="com.yahui.studyandroid.HtmlViewActivity"
            android:label="@string/htmlapp"
            android:launchMode="standard"
            ></activity>
        <activity 
            android:name="com.yahui.studyandroid.LoginActivity"
            android:label="@string/htmlapp"
            android:launchMode="standard"
            ></activity>
         <activity 
            android:name="com.yahui.studyandroid.SmartImgViewActivity"
            android:label="@string/htmlapp"
            android:launchMode="standard"
            ></activity>
         <activity 
            android:name="com.yahui.studyandroid.MutiDownloaderActivity"
            android:launchMode="standard"
            ></activity>
    </application>

</manifest>
